ec-corporate-header-mobile-nav
----field----
field: {E3CDCF5B-6FD6-48B1-A02F-797D3BCD50D8}
name: Template
key: template
content-length: 2070

{{ include "ec-corporate-format-id-function" }}

{{ 
    $mobileNav = $1.mobileNav

    for $level1Child in (get_children_in_language $mobileNav)
        $hasChildren = has_children_in_language $level1Child

        $title = $level1Child.HeaderTitle.raw
        if(!$title || $title == "")
            $title = $level1Child.NavigationTitle.raw
        end

}}
        <li class="rel-level1" role="menuitem">
{{
            $itemDetails = {
                item: $level1Child,
            }

}}
            <div class="navigation-title {{ if $hasChildren }} has-children chevron-primary-1-next-sm {{ end }}">
                {{ include 'ec-corporate-nav-output-mobile-top-level-item' $itemDetails }}
                
            </div>
            {{ if $hasChildren }}
                {{#Level 2 - Start}}
                <div role="group" aria-label="{{$title}}" aria-labelledby="{{ $level1Child.id | format_id | string.prepend "mobile-" }}">
                    <div class="nav-container">
                        <div class="head">
                            <div class="chevron-primary-1-prev-sm back-arrow"></div>
                            <h5 class="subnav-title"><span class="primary-underline">{{ $title }}</span></h5>
                        </div>
                        {{ for $column in (get_children_in_language $level1Child) }}

                            {{ include 'ec-corporate-nav-output-mobile-level-heading' $column }}
                            <ul role="menu">
                                {{ for $level2Child in (get_children_in_language $column) }}
                                    <li class="rel-level2" role="menuitem">
                                        {{ include 'ec-corporate-nav-output-mobile-level-item' $level2Child }}
                                    </li>
                                {{ end }}
                            </ul>
                        {{ end }}
                    </div>
                </div>
            {{ end }}
        </li>
    {{ end }}

