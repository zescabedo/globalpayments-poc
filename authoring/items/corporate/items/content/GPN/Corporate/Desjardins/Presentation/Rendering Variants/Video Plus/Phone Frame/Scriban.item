{{
        $v = get_general_link_info i_datasource "MP4VideoLink"
        $vUrl = ''
		$vLinkDesc = ''
        if($v)
            $vUrl = $v.url
			$vLinkDesc = i_datasource.VideoCaption
        end
        $i_url = ""
        if(i_datasource.VideoPoster && i_datasource.VideoPoster.target)
            $i_url = "background-image:url(" + i_datasource.VideoPoster.target.media_url + ");"
        end
}}
<div class="phone phone-shadow-left" style="{{ $i_url }}">
<a title="{{$vLinkDesc }}" aria-label = "{{$vLinkDesc }}" href="#" class="modal-link overlay-source"  data-overlay-target="/modals/video-overlay?videoSrc={{  $vUrl }}">
	{{ if o_pagemode.is_experience_editor }}
	<video id="bgVideo" preload="true" muted="">
	{{ else }}
	<video id="bgVideo" preload="true" loop="" autoplay="" muted="">
	{{ end }}
	    <source src="{{ $vUrl }}" type="video/mp4">
	</video>
</a>
</div>

