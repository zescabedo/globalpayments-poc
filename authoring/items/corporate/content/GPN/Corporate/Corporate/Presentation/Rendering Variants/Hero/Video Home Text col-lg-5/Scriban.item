            <a href="{{ $cta.url }}" class="{{ $cta.styleclass}}">{{ $cta.text }}</a>
            </div>
          {{ end }}
        </div>
      </div>
      <div class="image-wrapper">
        <img src="{{$mobileImgUrl}}" alt="{{$mobileImgAlt}}" loading="lazy" class="img-fluid" />
      </div>
    </div>
