{{-
    if i_item.template_name == "Blog Content Page"

    $url = i_item.url
    $a = get_general_link_info i_item "ExternalLink" 
    $target="_self"
    $read= i_item.BlogReadTime
    $readLabel=" lecture minute"

    if $a != null && $a.url != ""
        $url = $a.url
        $target="_blank"
        $read=""
        $readLabel=""
    end
-}} 

<a title="{{ i_item.title.raw }}" href="{{ $url }}" target="{{ $target }}">
   <div class="blog_single">
    <div class="blog_img">
        <div class="image-focal-set initialized" style="background-image:url({{ i_item.BlogFeaturedImage.media_url }}&w=440);height:205px;background-size: cover;background-repeat: no-repeat;" data-mobxfp="{{ sc_field i_item 'Mobile X Point' }}" data-mobyfp="{{ sc_field i_item 'Mobile Y Point' }}" data-nonmobxfp="{{ sc_field i_item 'NonMobile X Point' }}" data-nonmobyfp="{{ sc_field i_item 'NonMobile Y Point' }}">
        </div>
    </div>
    <div class="blog_single_meta">
        <p class="blog_single_meta_readtext field-blogreadtime" style="font-weight:300;margin-left:auto; margin-right:0;">{{ $read }}
            <span>{{ $readLabel }}</span>
        </p>
    </div>
    <div class="blog_single_title">
        <p class="field-blog-title">{{ sc_field i_item 'Blog Title' }}
        </p>
    </div>
    <div class="blog_single_desc">
        <p class="field-blogsummary">{{i_item.BlogSummary}}
        </p>
    </div>
    </div>
</a>{{
      end -}}

  {{-
    if i_item.template_name == "Customer Story"
    $mediaUrl = sc_fullmediaurl i_item 'ArticlePromoImage' { Width: 440 }
      -}}

<a title="{{i_item.Name}}" href="{{i_item.url}}">
    <div class="blog_single">
    <div class="blog_img">
        <div class="image-focal-set initialized" style="background-image:url({{ $mediaUrl }}&w=440);height:205px;background-size: cover;background-repeat: no-repeat;" data-mobxfp="{{ sc_field i_item 'Mobile X Point' }}" data-mobyfp="{{ sc_field i_item 'Mobile Y Point' }}" data-nonmobxfp="{{ sc_field i_item 'NonMobile X Point' }}" data-nonmobyfp="{{ sc_field i_item 'NonMobile Y Point' }}">
        </div>
    </div>
    <div class="blog_single_meta">
        <p class="blog_single_meta_readtext field-blogreadtime" style="font-weight:300;margin-left:auto; margin-right:0;">{{ i_item.CaseStudyReadTime}}
            <span> minute read</span>
        </p>
    </div>
    <div class="blog_single_title">
        <p class="field-blog-title">{{ sc_field i_item 'Title' }}
        </p>
    </div>
    <div class="blog_single_desc">
        <p class="field-blogsummary">{{ sc_field i_item 'Content' }}
        </p>
    </div>
    </div>
</a>{{
      end }}
