name: Template
key: template
content-length: 2404

{{ include "ec-corporate-format-id-function" }}

{{ 
    $primaryNav = $1.primaryNav
}}

<div class="navigation navigation-gpn-main">
    <nav class="navbar navbar-nav navbar-expand">

        <ul class="navbar-nav mr-auto level1" role="menubar" {{ if false }} data-bs-auto-close="true" {{ end }}>
        {{

            $level1Children = get_children_in_language $primaryNav

            for $level1Child in $level1Children
                $hasChildren = has_children_in_language $level1Child
        }}
                <li class="nav-item {{ if $hasChildren }}dropdown{{ end }} js-nav-level-one" role="menuitem" {{ if $hasChildren }}data-toggle="dropdown"{{ end }}>
        {{
                $itemDetails = {
                    item: $level1Child,
                }

                include 'ec-corporate-nav-output-top-level-item' $itemDetails

                if $hasChildren
                    if $level1Child.DropdownPosition 
                        if $level1Child.DropdownPosition != ""
                            $dropdownPosition = $level1Child.DropdownPosition
                        end
                    end
        }}
                    <div class="dropdown-menu" {{- if $dropdownPosition -}} style="left: {{ $dropdownPosition }};" {{- end -}}  aria-labelledby="{{ $level1Child.id | format_id }}">
                        <div class="submenu">
                            {{ for $column in (get_children_in_language $level1Child) }}
                                <div class="submenu-container" role="group">

                                    {{ include 'ec-corporate-nav-output-column-level-heading' $column }}

                                    <ul class="level2" role="menu">
                                        {{ for $level2Child in (get_children_in_language $column) }}
                                            <li role="menuitem">
                                                {{ include 'ec-corporate-nav-output-column-level-item' $level2Child }}
                                            </li>
                                        {{ end }}
                                    </ul>
                                </div>
                            {{ end }}
                        </div>
                    </div>
                {{ end }}
            {{ end }}
        </ul>
    </nav>
</div>
