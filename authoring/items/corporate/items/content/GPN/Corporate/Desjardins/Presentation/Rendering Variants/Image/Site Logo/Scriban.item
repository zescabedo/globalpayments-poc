       end
     if(o_language == "hu-HU")
          $lang = "/hu-hu"
       end
     if(o_language == "es-ES")
          $lang = "/es-es"
       end
}}

<a title="Logo" href="{{$lang}}" class="image-wrapper">
{{- if (o_pagemode.is_experience_editor_editing)
    -}}
        <span>[Edit Image here]</span>
        {{ i_datasource.Image }}

   {{else}}
        <img src='{{$mediaUrl}}' alt='{{$mediaAlt}}' loading="lazy"/>
   {{ end }}
</a>
