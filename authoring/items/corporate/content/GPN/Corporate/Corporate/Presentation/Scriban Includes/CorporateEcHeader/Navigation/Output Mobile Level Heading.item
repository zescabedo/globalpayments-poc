name: Template
key: template
content-length: 848

{{

    # Mobile Column level navigation heading
    $item = $1

    $a = get_general_link_info $item "NavigationLink"

    $title = $item.HeaderTitle.raw
    if(!$title || $title == "")
        $title = $item.NavigationTitle.raw
    end

    $aProps = ''
    if $a
        if $a.url && $a.url != "" && !o_pagemode.is_experience_editor_editing
            $aProps = "href='" + $a.url + "' title='" + $a.title + "' target='" + $a.target + "'" + "data-ea-zone='Header' data-ea-type='click' data-ea-link='" + $a.gatext + "'"
        end
    end
}}

{{ if $title }}
    {{ if $aProps && $aProps != "" }}
        <a {{ $aProps }} tabindex="0">  
            <div class="mobile-column-title">{{ $title }} </div>
        </a>
    {{ else }}
        <div class="mobile-column-title">{{ $title }} </div>
    {{ end }}
{{ end }}

