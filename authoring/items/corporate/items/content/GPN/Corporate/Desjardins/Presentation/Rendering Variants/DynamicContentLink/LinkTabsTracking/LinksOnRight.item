<div class="row dcl-inner">
    <div class="col-12 col-md-6 col-lg-5 dcl-container">
    {{
    $counter = 1
    for i_child in i_datasource.children
    $ph_link = "dynamiccontentlinks_" + $counter
    }}
    <div class="w-100 dcl">{{sc_placeholder $ph_link}} </div>{{
    $counter = $counter + 1
    end  }}
</div>
<ul class="col-12 col-md-6 col-lg-6 offset-lg-1 dcl-heading">
    {{
    for i_child in i_datasource.children
    linkInfo = (get_general_link_info i_child 'DynamicContentLink')
    $aItemProps = ''
    if linkInfo && linkInfo.url && linkInfo.url != ""
    $aItemProps = "href='" + linkInfo.url + "' title='" + linkInfo.title + "' class= '" + "p-lg link-dynamic-content " + linkInfo.styleclass + "'target='" + linkInfo.Target + "'" + "data-ea-zone='Body' data-ea-type='click' data-ea-link='" + linkInfo.gatext + "'"
    end
    }}
    <li>
    <a {{ $aItemProps }}>{{linkInfo.text}}</a>
    </li>{{
    end  }}
</ul>
</div>
