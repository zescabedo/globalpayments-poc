----field----
field: {936E68B4-5346-4ADB-9B77-79771D0B40DC}
name: Template
key: template
content-length: 4568

<div class="footer-content row">

    <div class="col-12 col-lg-3 mt-3">
        <div class="logo"></div>
        <div class="d-none">
            <p><strong>{{ i_datasource.RegionSelectorTitle }}</strong></p>
            {{ $a = get_general_link_info i_datasource "RegionSelectorModal" }}     
            <a class="btn btn-white overlay-source icon-globe" href="{{ $a.url }}" id="region-selector" data-ea-zone="Footer" data-ea-type="click" data-ea-link="{{ $a.gatext }}">
                {{ i_datasource.LanguageName }}
            </a>
        </div>
    </div>

    {{ for i_links in i_datasource.FooterLinks.children }}
    <div class="component link-list col-12 col-md-4 col-lg-4 mt-3">
        <div class="component-content">
            <div class="row">
                <div class="col-12 col-lg-6">
                    <ul>
                    {{ for i_link in i_links.children
                            if for.index > 5; break; end;
                    }}
                        <li>
                            {{ $a = get_general_link_info i_link "Link" }}                    
                            <a href="{{ $a.url }}" title="{{ $a.title }}" target="{{ $a.target }}" data-ea-zone="Footer" data-ea-type="click" data-ea-link="{{ $a.gatext }}">
                            {{ $a.text }}
                            {{if $a.linktype == "external"}}
                            <i class="icon-external-link ml-1"></i>
                            {{ end }}
                            </a>
                        </li>
                    {{ end }}
                    </ul>
                </div>
                <div class="col-12 col-lg-6">
                    <ul>
                    {{ for i_link in i_links.children
                            if for.index <= 5; continue; end;
                    }}
                        <li>
                            {{ $a = get_general_link_info i_link "Link" }}                    
                            <a href="{{ $a.url }}" title="{{ $a.title }}" target="{{ $a.target }}" data-ea-zone="Footer" data-ea-type="click" data-ea-link="{{ $a.gatext }}">
                            {{ $a.text }}
                            {{if $a.linktype == "external"}}
                            <i class="icon-external-link ml-1"></i>
                            {{ end }}
                            </a>
                        </li>
                    {{ end }}
                    </ul>
                </div>
            </div>
        </div>
    </div>
    {{ end }}

    <div class="component link-list col-12 col-md-4 col-lg-2 offset-lg-1 mt-3">
        <div class="component-content">
            <p class="h6-style">{{ i_datasource.CTA1Title }}</p>
            <p class="p-sm">{{ i_datasource.CTA1Text }}</p>
            {{ $a = get_general_link_info i_datasource "CTA1Link" }}                    
            <a class="btn btn-outline" href="{{ $a.url }}" title="{{ $a.title }}" target="{{ $a.target }}" data-ea-zone="Footer" data-ea-type="click" data-ea-link="{{ $a.gatext }}">{{ $a.text }}</a>
        </div>
    </div>

    <div class="component link-list col-12 col-md-4 col-lg-2 mt-3">
        <div class="component-content">
            <p class="h6-style">{{ i_datasource.SocialIconsTitle }}</p>
            <ul>
            {{ for i_image in i_datasource.SocialIcons.children }}
            {{
                $mediaUrl = i_image.Image.media_url
                $w = "24"
                $h = "25"
                $mediaAlt = ''
                if(i_image.Image)
                  $mediaAlt = i_image.Image.alt
                end
             }}
                <li>
                    {{ $a = get_general_link_info i_image "TargetUrl" }}
                    <a href="{{ $a.url }}" title="{{ $a.title }}" target="{{ $a.target }}" data-ea-zone="Footer" data-ea-type="click" data-ea-link="{{ $a.gatext }}" data-sc-goal="C907B6D1-C87D-41EC-A379-D7B1F9A48726">
                    {{- if (o_pagemode.is_experience_editor_editing)
                    -}}
                        <span>[Edit Image here]</span>
                        {{ i_image.Image }}

                    {{else}}
                        <img src='{{$mediaUrl}}' alt='{{$mediaAlt}}' loading="lazy" width="{{$w}}" height="{{$h}}" />
                    {{ end }} <span class="ml-1">{{ $a.text }}</span>
                    </a>
                </li>
            {{ end }}
            </ul>
        </div>
    </div>

</div>
