        end
        $i_url = ""
        if(i_datasource.VideoPoster && i_datasource.VideoPoster.target)
            $i_url = i_datasource.VideoPoster.target.media_url
        end
     }}
<section class="video-wrap static-autoplay">
<div class="tint"></div>
  <video playsinline muted loop autoplay class="video-background h-100 video_static"  style="height: 101%!important;" poster="{{ $i_url }}" title="Decorative video">
    {{ if !o_pagemode.is_experience_editor_editing }}
      <source src="{{ $vUrl }}" type="video/webm">
      <source src="{{ $vUrl }}" type="video/mp4">
    {{ end }}
  </video>
</section>
<style>
.video-wrap { height: 100%!important; }
.video_static{
    width: 100%;
    object-fit: cover;
}

.on-page-editor #video_static, .on-page-editor .video-wrap {
    max-height: 900px;
}

.video-plus .tint{
z-index: 0;
}
</style>
