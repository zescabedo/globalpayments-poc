{{ if (!o_pagemode.is_experience_editor_editing) 

  p_bgurl = sc_follow i_datasource "HeroBackgroundImage" | sc_medialink
  p_sm_bgurl = sc_follow i_datasource "HeroSmImage" | sc_medialink

  if(p_bgurl && p_bgurl != "" && p_bgurl != "#")
    p_md_bgurl = p_bgurl
    p_bgurl = "background-image:url('" +p_bgurl + "')"
  else
     p_bgurl = ""
     p_md_bgurl = p_bgurl
  end

  if(p_sm_bgurl && p_sm_bgurl != "" && p_sm_bgurl != "#")
    p_sm_bgurl = p_sm_bgurl
  else
     p_sm_bgurl = ""
  end

  i_targetBgSize = i_datasource.BackgroundSize
 
  if(i_targetBgSize && i_targetBgSize != "")
    p_bgsize = sc_follow i_datasource "BackgroundSize" | sc_field "Value"
    p_bgsize = "background-size:" + p_bgsize  + ";"
  else
     p_bgsize = ""
  end

  p_mobx = sc_raw i_datasource "Mobile X Point"
  p_moby =  sc_raw i_datasource "Mobile Y Point"
  p_nonmobx = sc_raw i_datasource "NonMobile X Point"
  p_nonmoby = sc_raw i_datasource "NonMobile Y Point"
 
 -}}
     <div class="hero-bg image-focal-set" style="{{p_bgurl}};{{p_bgsize}}" data-mobxfp="{{ p_mobx ?? '' }}"  data-mobyfp="{{ p_moby ?? '' }}" data-nonmobxfp="{{  p_nonmobx ?? '' }}"  data-nonmobyfp="{{ p_nonmoby ?? '' }}" data-mobilebg="{{ p_sm_bgurl }}" data-desktopbg="{{ p_md_bgurl }}"> 
{{end }}

