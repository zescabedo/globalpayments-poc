{{
    $url = get_absolute_url i_page
}}


<div class="social-media-sharing">
    <div class="social-media-links-container">
        <a href="https://www.linkedin.com/shareArticle?mini=true&url={{ $url }}" rel="nofollow" target="_blank" class="social-sharing-link social-sharing-linkedin" data-ea-zone="Social" data-ea-type="click" data-ea-link="{{ $url }} | Share on Linkedin">
            <img src="/Theme/dist/corporate/images/icons/linkedin.svg" alt="Linkedin Logo">
            <span class="sr-only">{{ sc_translate "Share on" }}</span> <span>Linkedin</span>
        </a>
        <a href="https://twitter.com/intent/tweet?text={{ $url }}" rel="nofollow" target="_blank" class="social-sharing-link social-sharing-twitter" data-ea-zone="Social" data-ea-type="click" data-ea-link="{{ $url }} | Share on Twitter">
            <img src="/Theme/dist/corporate/images/icons/twitter.svg" alt="X (Twitter) Logo">
            <span class="sr-only">{{ sc_translate "Share on" }}</span> <span>X (Twitter)</span>
        </a>
        <a href="https://www.facebook.com/sharer/sharer.php?u={{ $url }}" rel="nofollow" target="_blank" class="social-sharing-link social-sharing-facebook" data-ea-zone="Social" data-ea-type="click" data-ea-link="{{ $url }} | Share on Facebook">
            <img src="/Theme/dist/corporate/images/icons/facebook.svg" alt="Facebook Logo">
            <span class="sr-only">{{ sc_translate "Share on" }}</span> <span>Facebook</span>
        </a>
    </div>
</div>
