{{
    $i_url = ""
    if(i_datasource.PromoIcon && i_datasource.PromoIcon.target)
        $i_url = "background-image:url(" + i_datasource.PromoIcon.target.media_url + ");"
    end
    $bg_url = ""
    if(i_datasource.PromoIcon2 && i_datasource.PromoIcon2.target)
        $bg_url = "background-image:url(" + i_datasource.PromoIcon2.target.media_url + ");"
    end

    p_mobx = sc_raw i_datasource "Mobile X Point"
    p_moby =  sc_raw i_datasource "Mobile Y Point"
    p_nonmobx = sc_raw i_datasource "NonMobile X Point"
    p_nonmoby = sc_raw i_datasource "NonMobile Y Point"
}}

<div class="outer">
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-lg-7 component rich-text promo-text">
                {{ sc_field i_datasource 'PromoText' }}
            </div>
            {{ if (o_pagemode.is_experience_editor_editing) }}
                <div class="bg-img" style="{{ $bg_url }}"></div>
            {{ else }}
                <div class="bg-img image-focal-set" style="{{ $bg_url }}" data-mobxfp="{{ p_mobx ?? '' }}"  data-mobyfp="{{ p_moby ?? '' }}" data-nonmobxfp="{{  p_nonmobx ?? '' }}"  
                data-nonmobyfp="{{ p_nonmoby ?? '' }}"></div>
            {{ end }}
            <div class="col img-animate" style="{{ $i_url }}"></div>
            <div class="img-sm">
                <div class="img img-fluid" style="{{ $i_url }}"></div>
            </div>
        </div>
    </div>
</div>
