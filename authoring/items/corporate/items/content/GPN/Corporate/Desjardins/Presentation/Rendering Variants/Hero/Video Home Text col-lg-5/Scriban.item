{{ if (!o_pagemode.is_experience_editor_editing) 
  $cta = get_general_link_info i_datasource "HeroLink"
  $mobileImgUrl = ''
  $mobileImgAlt = ''
    if(i_datasource.HeroSmImage && i_datasource.HeroSmImage.media_url != "")
      $mobileImgUrl = i_datasource.HeroSmImage.media_url
      $mobileImgAlt = i_datasource.HeroSmImage.alt
    end
  $v = get_general_link_info i_datasource "HeroMovie"
        $videoUrl = ''
        if($v)
            $videoUrl = $v.url
        end
 -}}
{{end }}
<div class="hero-bg image-focal-set">
  <div class="container position-relative">
    <div class="row h-100 hero-container">
      <div class="col-12 promo-left mt-2 mt-md-4 mt-lg-7">
        <div class="promo-text">
          <div>
            <div class="col-12 col-lg-5 p-0 field-herotext">{{ sc_field i_datasource 'HeroText' }}</div>
            <div class="col-12 col-lg-5 p-0 field-herotext2">{{ sc_field i_datasource 'HeroText2' }}</div>
            <div class="col-12 col-lg-5 p-0 field-herotext3">{{ sc_field i_datasource 'HeroText3' }}</div>
          </div>
          {{ if $a != null && $a.url != "" }}
            <div class="field-herolink">
            <a href="{{ $cta.url }}" class="{{ $cta.styleclass}}">{{ $cta.text }}</a>
            </div>
          {{ end }}
        </div>
      </div>
      <div class="image-wrapper">
        <img src="{{$mobileImgUrl}}" alt="{{$mobileImgAlt}}" loading="lazy" class="img-fluid" />
      </div>
    </div>
    <div class="promo-image text-right video-wrapper" style="height: 640px;">
      <video muted loop playsinline autoplay height="640px" width="779px" class="decorative-video" title="Decorative background video">
        {{ if !o_pagemode.is_experience_editor_editing }}
        <source src="{{$videoUrl}}" type="video/mp4">
         <!--source src="https://gpnprodsxavideo.azureedge.net/sxa/videos/corporate/HomeHeroCropV2.mp4" type="video/mp4"-->
        {{ end }}
      </video>
    </div>
  </div>
</div>

