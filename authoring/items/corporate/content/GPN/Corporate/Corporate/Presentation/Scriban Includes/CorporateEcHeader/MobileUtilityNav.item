name: Template
key: template
content-length: 1139

{{ 
    $links = $1.links
}}

<li class="rel-level1" role="menuitem">
    <ul class="mobile-utility-nav-list">
        {{
            for $link in (get_children_in_language $links)
                
                $aItem = get_general_link_info $link 'Link'

                if $aItem.text && !string.empty $aItem.text.raw

                    $aItemProps = ''
                    if $aItem && $aItem.url && $aItem.url != ""
                        $aItemProps = "href='" + $aItem.url + "' title='" + $aItem.title + "' class='" + $aItem.styleclass + "'target='" + $aItem.target + "'" + "data-ea-zone='Header' data-ea-type='click' data-ea-link='" + $aItem.gatext + "'"
                    end
                    if $aItem && $aItem.url && $aItem.url != "" 
            }}
                        <li>
                            <div class="mobile-utility-nav-title">
                                <a {{ $aItemProps }}>{{$aItem.text}}</a>
                            </div>
                        </li>
            {{ 
                    end
                end 
            end
        }}
    </ul>
</li>
