----field----
field: {E3CDCF5B-6FD6-48B1-A02F-797D3BCD50D8}
name: Template
key: template
content-length: 1553

{{ include "format-id-function" }}

{{ 
    $mobileNav = $1.mobileNav

    for $level1Child in $mobileNav.children
        $hasChildren = $level1Child.children.count > 0

        $title = $level1Child.HeaderTitle.raw
        if(!$title || $title == "")
            $title = $level1Child.NavigationTitle.raw
        end

}}
        <li class="rel-level1" role="menuitem">
{{
            $itemDetails = {
                item: $level1Child,
            }

}}
            <div class="navigation-title {{ if $hasChildren }} has-children chevron-primary-1-next-sm {{ end }}">
                {{ include 'nav-output-mobile-top-level-item' $itemDetails }}
                
            </div>
            {{ if $hasChildren }}
                {{#Level 2 - Start}}
                <div role="group" aria-label="{{$title}}" aria-lablledby="{{ $level1Child.id | format_id }}">
                    <div class="head">
                        <div class="chevron-primary-1-prev-sm back-arrow"></div>
                        <h5 class="subnav-title"><span class="primary-underline">{{ $title }}</span></h5>
                    </div>
                    <ul role="menu">
                        {{ for $level2Child in $level1Child.children }}
                        <li class="rel-level2" role="menuitem">
                            {{ include 'nav-output-mobile-level-item' $level2Child }}
                        </li>
                    {{ end }}
                </div>
            {{ end }}
        </li>
    {{ end }}

