name: Template
key: template
content-length: 1262

{{
        $mediaUrl = i_datasource.Image.media_url
        $mediaUrlSm = i_datasource.ImageSm.media_url

        # for image rendering support, css overrides - this holds good for both mobile and desktop default to src attributes
        $w = i_datasource.Image.width
        $h = i_datasource.Image.height

        $mediaAlt = ''
        if(i_datasource.Image)
            $mediaAlt = i_datasource.Image.alt
        end
        if($mediaAlt == "")
            $mediaAlt = i_datasource.Image.Caption
        end

        # default aspect ratio 16:9 in case width height not set on img esp svgs
        if($w == "")
            $w = "800"
        end
        if($h == "")
            $h = "450"
        end
}}

<div class="image-wrapper">
   {{- if (o_pagemode.is_experience_editor_editing) 
    -}}
        <span>[Edit Image here]</span>
        {{ i_datasource.Image }}
        <span>[Edit Image Small here]</span>
        {{ i_datasource.ImageSm }}
        
   {{else}}
        <picture>
            <source media='(max-width: 1023px)' srcset='{{$mediaUrlSm}}'>
            <img src='{{$mediaUrl}}' alt='{{$mediaAlt}}' loading="lazy" width="{{$w}}" height="{{$h}}" class="w-100" />
        </picture>
   {{ end }}
</div>
