{{ 

include "format-id-function"

$secondaryMenu = $1.secondaryMenu

for $link in $secondaryMenu.children

    $aItem = get_general_link_info $link 'Link'
    $aItemProps = ''
    if $aItem && $aItem.url && $aItem.url != ""
        $aItemProps = "href='" + $aItem.url + "' title='" + $aItem.title + "' class='" + $aItem.styleclass + "'target='" + $aItem.target + "'" + "data-ea-zone='Header' data-ea-type='click' data-ea-link='" + $aItem.gatext + "'"
    end

    $title = $aItem.text
    
    if $aItem && $aItem.url && $aItem.url != ""
}}

        <li class="nav-item " role="menuitem">
            <a class="nav-link" {{ $aProps }}>
                <span class="top-navigation-title" data-content="{{ $title }}">{{ $title }}</span>
            </a>
        </li>


    {{ end }}
{{ end }}

