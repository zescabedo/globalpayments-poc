name: Template
key: template
content-length: 682

{{
    $links = $1.links
    $title = $1.title
    $containerCssClass = $1.containerCssClass
}}

<div class="{{ $containerCssClass }}">
    <h3 class="search-prompt-title">{{ sc_translate $title }}</h3>
    <ul>
        {{ 

            for $link in $links
                $linkInfo = get_general_link_info $link "Link"
                if $linkInfo
                    $url = $linkInfo.url
                    $text = $linkInfo.text
                    $target = $linkInfo.target
        }}

                    <li><a href="{{ $url }}" target="{{ $target }}">{{ $text }}</a></li>
        {{
                end
            end
        }}
    </ul>
</div>

