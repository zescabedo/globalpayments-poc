<div class="row dcl-inner">
    <ul class="col-12 col-lg-4 dcl-heading">
        {{
            for i_child in i_datasource.children
            linkInfo = (get_general_link_info i_child 'DynamicContentLink')
        }}
        <li>
            <div class="field-heading">
                <a><h5>{{ linkInfo.text }}</h5></a>
                <div class="icon-handle"></div>
            </div>
        </li>{{
            end  }}
    </ul>
    <div class="col-12 col-lg-7 offset-lg-1 dcl-container">
    {{
        $counter = 1
        for i_child in i_datasource.children
        $ph_link = "dynamiccontentlinks_" + $counter
    }}
        <div class="w-100 h-100 dcl">
            <div class="getHeight" data-height="{{ i_child.DynamicContentHeight.raw }}">{{sc_placeholder $ph_link}} 
            </div>
        </div>{{
        $counter = $counter + 1
        end  }}
    </div>
</div>
