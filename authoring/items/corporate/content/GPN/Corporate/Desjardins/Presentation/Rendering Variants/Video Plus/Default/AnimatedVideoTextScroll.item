<div class="video-panel textScrollOverVideo" id="videoTextScrollPanl">
<div class="tint"></div>
    {{
        $v = get_general_link_info i_datasource "MP4VideoLink"
        $vUrl = ''
        $vTitle = ''
        if($v)
            $vUrl = $v.url
            $vTitle = $v.title
        end
        $i_url = ""
        if(i_datasource.VideoPoster && i_datasource.VideoPoster.target)
            $i_url = i_datasource.VideoPoster.target.media_url
        end
     }}
    <section class="video-wrap">
    <div class="top-mask"></div>
    <div class="left-mask"></div>
    <div class="right-mask"></div>
        <video muted loop playsinline class="video-background" id="video" poster="{{ $i_url }}" title="{{ $vTitle }}">
            {{ if !o_pagemode.is_experience_editor_editing }}
            <source src="{{ $vUrl }}" type="video/mp4">
            {{ end }}
        </video>
    </section>
    <div class="col-12 px-0">
    {{sc_raw i_datasource "VideoOverlayText"}}
    </div>
</div>
