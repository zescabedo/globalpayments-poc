----field----
field: {936E68B4-5346-4ADB-9B77-79771D0B40DC}
name: Template
key: template
content-length: 9151

{{ if (o_model.relative_level == 1) }}

<ul class="clearfix ul-level-{{ o_model.relative_level }} test-default" role="menubar">
    {{ for i_level1 in o_model.children }}
    <li class="{{i_level1.css_classes | array.join " "}} rel-level{{ o_model.relative_level }}" role="presentation">
        <div class="navigation-title">
            {{
                $a = get_general_link_info i_level1.page_item "NavigationLink"

                $l1_title = i_level1.page_item.HeaderTitle
                if(!$l1_title || $l1_title == "")
                    $l1_title = i_level1.page_item.NavigationTitle
                end

                $aProps = ''
                if $a
                    if $a.url && $a.url != ""
                        $aProps = "href='" + $a.url + "' title='" + $a.title + "' target='" + $a.target + "'" + "data-ea-zone='Header' data-ea-type='click' data-ea-link='" + $a.gatext + "'"
                    else
                        $aProps = "title='" + $a.title + "' data-ea-zone='Header' data-ea-type='parent tab click' data-ea-link='" + $l1_title.raw + "'"
                    end
                end
            }}
            <a {{ $aProps }} role="menuitem" aria-haspopup="{{i_level1.children.size > 0}}">{{ $l1_title }}</a>
        </div>
        {{ if i_level1.children.size > 0 }}
        {{#Level 2 - Start}}
        <ul class="clearfix nav-submenu" role="menu" aria-label="{{$l1_title}}">
            <div class="container">
                <div class="row">
                    {{
                        $first_item = i_level1.page_item
                        $item_class_id = sc_follow $first_item "NavigationClass"
                        $item_class = ""
                        if($item_class_id)
                            $item_class = sc_follow $first_item "NavigationClass" | sc_field "Value"
                        end
                    }}
                    {{if $item_class == "nav-4col-links-stdout"}}
                    {{ for i_level2 in i_level1.children
                        if for.index > 4; break; end;
                    }}
                    <div class="col-12 col-md-3 bg-white navigation-title">
                        {{
                            $a2 = get_general_link_info i_level2.page_item "NavigationLink"

                            $p_title = i_level2.page_item.HeaderTitle
                            if(!$p_title || $p_title == "")
                                $p_title = i_level2.page_item.NavigationTitle
                            end

                            $a2Props = ''
                            if $a2 && $a2.url && $a2.url != ""
                                $a2Props = "href='" + $a2.url + "' title='" + $a2.title + "' target='" + $a2.target + "'" + "data-ea-zone='Header' data-ea-type='click' data-ea-link='" + $a2.gatext + "'"
                            end
                            
                        }}
                        <h5 class="subnav-title primary-underline"><a {{ $a2Props }}>{{$p_title}}</a></h5>
                        {{i_level2.page_item.HeaderSummary}}
                        <ul role="presentation">
                            <li class="rel-level2" role="presentation">
                                <ul>
                                    {{#Level3 - Start}}
                                    {{ for i_level3 in i_level2.children
                                    }}
                                    <li class="{{i_level3.css_classes | array.join " "}}" role="none">
                                        <div class="navigation-title">
                                            {{
                                            
											
											
											$a3 = get_general_link_info i_level3.page_item "NavigationLink"
                                            
											$p3_title = i_level3.page_item.NavigationTitle
                                            if(!$p3_title || $p3_title == "")
                                                $p3_title = i_level3.page_item.NavigationTitle
                                            end
											
											$a3Props = ''
                                            if $a3 && $a3.url && $a3.url != ""
                                                $a3Props = "href='" + $a3.url + "' title='" + $p3_title + "' target='" + $a3.target + "'" + "data-ea-zone='Header' data-ea-type='click' data-ea-link='" + $a3.gatext + "'"
                                            end
                                            

                                            }}
                                            <a role="menuitem" {{ $a3Props }}>{{$p3_title}}</a>
                                            {{$head_summary = i_level3.page_item.HeaderSummary}}
                                            {{if ($head_summary && $head_summary != "")}}
                                            <p class="field-headersummary">{{$head_summary}}</p>
                                            {{ else }}
                                            <div class="l3-spacer"></div>
                                            {{ end }}
                                        </div>
                                    </li>
                                    {{ end }}
                                    {{#Level3 - End}}
                                </ul>
                            </li>
                        </ul>
                    </div>
                    {{ end }}
                    
                    {{else}}
                    
                    <div class="col-12 col-lg-8 bg-white">
                        <ul class="col-12" role="presentation">
                            <li class="rel-level2" role="presentation">
                                <ul class="make-2cols-of-4" role="presentation">
                                    {{#Level3 - Start}}
                                    {{ for i_level3 in i_level1.children
                                    }}
                                    <li class="{{i_level3.css_classes | array.join " "}}" role="presentation">
                                        <div class="navigation-title primary-underline">
                                            {{
                                                $a4 = get_general_link_info i_level3.page_item "NavigationLink"
                                                $a4Props = ''
                                                if $a4 && $a4.url && $a4.url != ""
                                                    $a4Props = "href='" + $a4.url + "' title='" + $a4.title + "' target='" + $a4.target + "'" + "data-ea-zone='Header' data-ea-type='click' data-ea-link='" + $a4.gatext + "'"
                                                end
                                                $p4_title = i_level3.page_item.HeaderTitle
                                                if(!$p4_title || $p4_title == "")
                                                $p4_title = i_level3.page_item.NavigationTitle
                                                end

                                            }}
                                            <a role="menuitem" {{ $a4Props }}>{{$p4_title}}</a>
                                            {{$head_summary = i_level3.page_item.HeaderSummary}}
                                            {{if ($head_summary && $head_summary != "")}}
                                            <p class="field-headersummary">{{$head_summary}}</p>
                                            {{ end }}
                                        </div>
                                    </li>
                                    {{ end }}
                                    {{#Level3 - End}}
                                </ul>
                            </li>
                        </ul>
                    </div>
                    {{ end }}
                </div>
            </div>
        </ul>
        {{#Level2 - End}}
        {{ end }}
    </li>
    {{ end }}
    <li class="level1 submenu odd rel-level1 search-toggle" role="none">
        <div class="navigation-title">
            <a title="Search" role="menuitem" aria-haspopup="true">
                <span class="visuallyhidden">Search</span>
                <div class="search-toggle-icon"></div>
            </a>
        </div>
        <div class="clearfix nav-submenu" style="background-color: transparent;" role="menu" aria-label="Search">
            <div class="container">
                <div class="row">
                    {{sc_placeholder "header-nav-content"}}
                </div>
            </div>
        </div>
    </li>
</ul>
<a class="btn btn-sm btn-outline-primary-1 overlay-source float-right modal-trigger d-none" data-overlay-target="/modals/signup-homepage" title="Signup Homepage" rel="nofollow" data-ea-zone="Header" data-ea-type="click" data-ea-link="/ | modal trigger | " hidden>Sign up</a>
{{ end }}
