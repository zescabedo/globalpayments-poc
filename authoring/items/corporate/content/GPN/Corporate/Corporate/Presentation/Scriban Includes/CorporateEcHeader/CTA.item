{{ 

    $page = $1.page
    $linkField = $1.linkField
    $item = $1.item

    # get sign up modal if it exists on the page
    $a = get_general_link_info $page "Signup Modal"
    $atext = ""
	if($a!= null)
        if($a.description == "")
	        $b = get_general_link_info $item $linkField
	        $atext = $b.text
        else
	        $atext = $a.text	
        end
	end

    # if it doesn't, then get the general one from the header datasource
    if !$a || !$a.url || $a.url == ""
        $a = get_general_link_info $item $linkField
	    $atext = $a.text
    end
}}

<a class="btn btn-sm btn-cta-secondary overlay-source float-right" data-overlay-target="{{$a.url}}?theme=split-base-white-theme&type=fullscreen" title="{{ $a.title }}" rel="nofollow" data-ea-zone="Header" data-ea-type="click" data-ea-link="{{ $a.gatext }}">
{{ $atext }}
</a>
